<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Logincontrol</servlet-name>
        <servlet-class>controller.Logincontrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeControl</servlet-name>
        <servlet-class>controller.HomeControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkControl</servlet-name>
        <servlet-class>controller.MarkControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AttendanceControl</servlet-name>
        <servlet-class>controller.AttendanceControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewAttenControl</servlet-name>
        <servlet-class>controller.ViewAttenControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logincontrol</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeControl</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkControl</servlet-name>
        <url-pattern>/mark</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AttendanceControl</servlet-name>
        <url-pattern>/attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewAttenControl</servlet-name>
        <url-pattern>/viewatten</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
